<?xml version="1.0" encoding="UTF-8"?>

<odoo>

        <record id="brutal_force_mail_server" model="ir.config_parameter">
            <field name="key">BRUTAL_FORCE_MAIL_SERVER</field>
            <field name="value">smtp.example.com</field>
        </record>

        <record id="brutal_force_mail_from" model="ir.config_parameter">
            <field name="key">BRUTAL_FORCE_MAIL_FROM</field>
            <field name="value">example@example.com</field>
        </record>

        <record id="brutal_force_mail_from_pws" model="ir.config_parameter">
            <field name="key">BRUTAL_FORCE_MAIL_FROM_PWD</field>
            <field name="value">example_password</field>
        </record>

        <record id="brutal_force_mail_server_port" model="ir.config_parameter">
            <field name="key">BRUTAL_FORCE_MAIL_SERVER_PORT</field>
            <field name="value">465</field>
        </record>

        <record id="brutal_force_mail_to" model="ir.config_parameter">
            <field name="key">BRUTAL_FORCE_MAIL_TO</field>
            <field name="value">['example_recipient@example.com']</field>
        </record>

</odoo>