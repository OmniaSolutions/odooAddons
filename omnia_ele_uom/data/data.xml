        </record>        
        
    <!-- UOM.UOM -->
    <!-- Ampere -->
	    <record id="omnia_uom_ampere" model="uom.uom">
	        <field name="category_id" ref="omnia_uom_categ_current"/>
	        <field name="name">Ampere</field>
	        <field name="factor" eval="1.0"/>
	        <field name="uom_type">reference</field>
	    </record>
	    <record id="omnia_uom_milliamper" model="uom.uom">
	        <field name="category_id" ref="omnia_uom_categ_current"/>
