<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
    <!-- UOM Categories -->
	    <record id="omnia_uom_categ_current" model="uom.category">
	        <field name="name">Current</field>
	    </record>
	    
        <record id="omnia_uom_categ_voltage" model="uom.category">
            <field name="name">Voltage</field>
        </record>
        <record id="omnia_uom_categ_electrical_resistance" model="uom.category">
