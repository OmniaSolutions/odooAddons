<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mrp_production_workcenter_form_view_inherit_user_ids" model="ir.ui.view">
            <field name="model">mrp.workorder</field>
            <field name="inherit_id" ref="mrp.mrp_production_workorder_form_view_inherit"/>
            <field name="arch" type="xml">
                <field name="time_ids" position="after">
                	<group>
                    	<field name ="user_ids" widget="many2many_tags"/>
                   	</group>
                </field>
            </field>
        </record>

	    <record id="mrp_production_workorder_tree_view_wo_machine" model="ir.ui.view">
	        <field name="name">mrp.production.work.order.tree.wo.machine</field>
	        <field name="model">mrp.workorder</field>
	        <field name="priority" eval="15"/>
	        <field name="inherit_id" ref="mrp.mrp_production_workorder_tree_editable_view"/>
	        <field name="arch" type="xml">
	            <field name="state" position="before">
	                <field name="user_ids" widget="many2many_tags"/>
	            </field>
	        </field>
	    </record>
   
    </data>
</odoo>