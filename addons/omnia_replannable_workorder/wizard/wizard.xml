<odoo>
    <data> 
        <record id="act_change_raw_material_view" model="ir.ui.view">
            <field name="name">act.change._raw.material.view</field>
            <field name="model">change_product_move_wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
              <form string="Change Raw Material">
                  <group>
                  	<field name="warning_message" nolabel="1" colspan="2" readonly="1"/>
                    <field name="product_id"/>
                  </group>
                 <footer>
                    <button name="changeProduct" string="Change" type="object" class="btn-primary"/>
                    <button special="cancel" string="Close"/>
                 </footer>
              </form>
            </field>
        </record>
    </data>
</odoo>